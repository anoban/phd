<SimulationModelIncludeFile
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../scripts/XML/SimulaXMLSchema.xsd">
	<!--PlantTemplate -->
	<SimulaDirective path="/plantTemplate">
		<SimulaVariable
			name="rootSecondaryPotentialCarbonSinkForGrowth_storageRoots"
			unit="g" function="plantTotal" />
		<SimulaVariable name="rootRespiration_storageRoots"
			unit="g" function="plantTotalRatesRootFraction" />

	</SimulaDirective>
	<SimulaDirective path="/hypocotylTemplate">

		<SimulaVariable
			name="rootSystemSecondaryPotentialCarbonSinkForGrowth_storageRoots"
			unit="g" function="rootSystemTotalRates">
			0.
			<SimulaConstant name="includedRootClasses"
				type="string"> storageRoot
			</SimulaConstant>
		</SimulaVariable>
		<SimulaVariable
			name="rootSystemRespiration_storageRoots" unit="g"
			function="rootSystemTotalRates">
			0.
			<SimulaConstant name="includedRootClasses"
				type="string"> storageRoot
			</SimulaConstant>
		</SimulaVariable>


	</SimulaDirective>
	<SimulaDirective path="/siblingRootTemplate">

		<SimulaVariable
			name="rootSystemSecondaryPotentialCarbonSinkForGrowth_storageRoots"
			unit="g" function="rootSystemTotalRates">
			0.
			<SimulaConstant name="includedRootClasses"
				type="string"> storageRoot
			</SimulaConstant>
		</SimulaVariable>
		<SimulaVariable
			name="rootSystemRespiration_storageRoots" unit="g"
			function="rootSystemTotalRates">
			0.
			<SimulaConstant name="includedRootClasses"
				type="string"> storageRoot
			</SimulaConstant>
		</SimulaVariable>

	</SimulaDirective>
</SimulationModelIncludeFile>
